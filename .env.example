# Google Calendar Authentication
# The calendar ID to fetch events from (usually your email)
GOOGLE_CALENDAR_ID=meeth@quizizz.com

# OAuth 2.0 Credentials (for production - Vercel)
# Get these from Google Cloud Console OAuth 2.0 Client
GOOGLE_OAUTH_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_OAUTH_CLIENT_SECRET=your-client-secret

# Upstash Redis (for production - Vercel)
# Auto-added by Vercel integration, or get from Upstash dashboard
UPSTASH_REDIS_REST_URL=https://your-db.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-token

# Service Account (for local development only - optional)
# Path to your service account JSON file
GOOGLE_SERVICE_ACCOUNT_PATH=./service-account.json

# Server port (local development)
PORT=3002
